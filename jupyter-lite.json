{
  "jupyter-lite-schema-version": 0,
  "jupyter-config-data": {
    "appName": "SpyTial CLRS - Interactive Notebooks",
    "appVersion": "0.1.0",
    "settingsOverrides": {
      "@jupyterlab/apputils-extension:themes": {
        "theme": "JupyterLab Light"
      }
    }
  },
  "LiteBuildConfig": {
    "contents": ["src/*.ipynb"],
    "ignore_sys_prefix": ["share/**"]
  }
}
